<Page x:Class="BKDelivery.CallCenter.AddressesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BKDelivery.CallCenter"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="AddressesPage"
      DataContext="{Binding AddressesVm, Source={StaticResource Locator}}">

    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button x:Name="AddAddressButton" Content="Add" Command="{Binding AddAddressCommand}" Height="30" Width="30" VerticalAlignment="Top"
        HorizontalAlignment="Left"/>
        </StackPanel>
    </DockPanel>
</Page>
